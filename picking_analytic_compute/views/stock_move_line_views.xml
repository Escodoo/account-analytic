<odoo>

  <record model="ir.ui.view" id="picking_analytic_view_move_line_tree">
    <field name="name">stock.move.line.tree (in stock_analytic)</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.view_move_line_tree" />
    <field name="arch" type="xml">
      <field name="qty_done" position="after">
        <field name="picking_line_cost" optional="hidden" />
      </field>
    </field>
  </record>

  <record model="ir.ui.view" id="picking_analytic_view_move_line_detailed_tree">
    <field name="name">stock.move.line.tree (in picking_analytic_compute)</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree" />
    <field name="arch" type="xml">
      <field name="qty_done" position="after">
        <field name="picking_line_cost" optional="hidden" />
      </field>
    </field>
  </record>


</odoo>
